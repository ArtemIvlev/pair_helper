          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  route,\n  round(percentile_cont(0.95) WITHIN GROUP (ORDER BY duration_ms)) AS p95_ms,\n  count(*) AS cnt\nFROM usage_events\nWHERE $__timeFilter(ts)\nGROUP BY route\nHAVING count(*) >= 50\nORDER BY p95_ms DESC\nLIMIT 10;",
